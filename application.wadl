    <wadl:resource pathname="/restapi/system/alert">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATAname[This call queries for a list of All Alert ids across all symmetrix arrays]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="type" style="query">
            <wadl:doc>
              <![CDATAname[Optional value that filters returned list to display
Alert Ids that has the following severity: ARRAY, PERFORMANCE, SERVER]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="description" style="query">
            <wadl:doc>
              <![CDATAname[Optional value that filters returned list to display
Alerts that have the specified description e.g. equal to "description=description1" or contains "description=< like>description"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="severity" style="query">
            <wadl:doc>
              <![CDATAname[Optional value that filters returned list to display
Alert Ids that has the following severity: NORMAL, INFORMATION, MINOR, WARNING, CRITICAL, FATAL]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="state" style="query">
            <wadl:doc>
              <![CDATAname[Optional value that filters returned list to display
Alert Ids that has the following state: NEW, ACKNOWLEDGED, CLEARED]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="created_date" style="query">
            <wadl:doc>
              <![CDATAname[Optional value that filters returned list to display
Alert Ids that are greater than(">1"), Less than("<1") or equal to the specified created_date "MMM-dd-yyyy HH:mm:ss"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="created_date_milliseconds" style="query">
            <wadl:doc>
              <![CDATAname[Optional value that filters returned list to display
Alert Ids that contain greater than(">1"), Less than("<1") or equal to the specified date in milliseconds]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="acknowledged" style="query">
            <wadl:doc>
              <![CDATAname[Optional value that filters returned list to display
Alert Ids that are acknowledged or not  (true/false)]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATAname[A list of Alert Ids]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="system:listAlertResult" xmlns:system="http://www.emc.com/em/2012/07/univmax/restapi/system"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
